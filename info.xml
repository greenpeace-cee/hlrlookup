<?xml version="1.0"?>
<extension key="hlrlookup" type="module">
  <file>hlrlookup</file>
  <name>HLR Lookup</name>
  <description>Track HLR lookup results</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/hlrlookup</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/hlrlookup</url>
    <url desc="Support">https://github.com/greenpeace-cee/hlrlookup/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-09-11</releaseDate>
  <version>1.0.0-beta.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Hlrlookup</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Hlrlookup_Upgrader</upgrader>
</extension>
